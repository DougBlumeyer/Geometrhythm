<span class='analysis-title TEDAS-tooltip'><h5>TEDAS</h5></span>

<div class="TEDAS_box">
  <% sum = 0 %>
  <% rhythm.get("durational_pattern") && rhythm.get("durational_pattern").forEach(function(interval_duration, index) { %>

    <div class="TEDAS_sq" ord="<%= sum %>" dur="<%= interval_duration %>"
          style="height: <%= interval_duration * heightPixelsUnit %>px;
                width: <%= interval_duration * widthPercentageUnit %>%;
                left: <%= (sum) * widthPercentageUnit %>%;
                bottom: 0px">
    </div>

    <% if (rhythm.get("contour")[index] === "+") { %>
      <i class="contour glyphicon glyphicon-circle-arrow-up" ord="<%= sum %>"
            style="left: <%= (((sum) + interval_duration) * widthPercentageUnit) - 5 %>%;
                   top: <%= ((max_height * 1.414) - interval_duration) * heightPixelsUnit %>px;">
      </i>
    <% } else if (rhythm.get("contour")[index] === "-") { %>
      <i class="contour glyphicon glyphicon-circle-arrow-down" ord="<%= sum %>"
            style="left: <%= (((sum) + interval_duration) * widthPercentageUnit) - 5 %>%;
                   top: <%= ((max_height * 1.414) - interval_duration) * heightPixelsUnit %>px;">
      </i>
    <% } else { %>
      <i class="contour glyphicon glyphicon-record" ord="<%= sum %>"
            style="left: <%= (((sum) + interval_duration) * widthPercentageUnit) - 5 %>%;
                   top: <%= ((max_height * 1.414) - interval_duration) * heightPixelsUnit %>px;">
      </i>
    <% } %>

    <% sum += interval_duration %>

  <% }) %>
</div>

<span class="info-icon" data-detailsviewname="templateBasic">i</span>
