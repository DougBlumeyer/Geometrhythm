<container>

  <div class='row'>

    <div class='col-md-2'>
      <interface oncontextmenu="return false;">
        tempo<br><input type="text" id="input-tempo" value="125"><br><br>
        <button class="btn btn-warning" id="play-pause"><i style="font-size: 12px" class="glyphicon glyphicon-play"></i>&nbsp;Play</button><br><br>
        <button class="btn btn-default btn-sm" id="invert"><i style="font-size: 12px" class="glyphicon glyphicon-adjust"></i>&nbsp;Invert</button>
        <button class="btn btn-default btn-sm" id="reverse">Reverse</button>
        <button class="btn btn-default btn-sm" id="clockwise">
          <i style="font-size: 12px" class="glyphicon glyphicon-refresh"></i>
        </button>
        <button class="btn btn-default btn-sm" id="c-clockwise">
          <i style="font-size: 12px" class="glyphicon glyphicon-refresh c-clockwise"></i>
        </button>
      </interface>
    </div>

    <div class='col-md-5'>
      <div class='wrapper' style='min-height: 480px'>
        <div class='rhythm-ring' oncontextmenu="return false;"></div>
        <canvas id='polygon-canvas' height='400' width='400'></canvas>
      </div>
    </div>

    <div class='col-md-4'>
      <div class="well scrollable">
        <div id="bb-info"></div>
      </div>
    </div>

  </div>

  <div class='row'>
    <div class='col-md-4 col-md-offset-1'>
      <div class='well bb-analysis' id='bb-analysis-basic'>
      </div>
    </div>
    <div class='col-md-4'>
      <div class='well bb-analysis' id='bb-analysis-interval'>
      </div>
    </div>
  </div>

  <div class='row'>
    <div class='col-md-4 col-md-offset-1'>
      <div class='well bb-analysis' id='bb-analysis-meter'>
      </div>
    </div>
    <div class='col-md-4'>
      <div class='well bb-analysis' id='bb-analysis-onset'>
      </div>
    </div>
  </div>

  <div class='row'>
    <div class='col-md-4 col-md-offset-1'>
      <div class='well bb-analysis' id='bb-analysis-evenness'>
      </div>
    </div>
    <div class='col-md-4'>
      <div class='well bb-analysis' id='bb-analysis-symmetry'>
      </div>
    </div>
  </div>

</container>
