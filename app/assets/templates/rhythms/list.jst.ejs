<container>

  <div class='row'>

    <div class='col-md-7 col-md-offset-1'>
      <div class='subview-version'>
      </div>
    </div>

    <div class='col-md-3'>
      <br>
      <button class="btn btn-warning btn-lrg" style="float: right" id="return-to-root">back to home</button>
      <br><br><br><br>
      <i class="glyphicon glyphicon-search"></i>&nbsp;&nbsp;&nbsp;<strong style='color: #57CD4A'>COMING SOON</strong>
      <br><span style='color: #57CD4A'>over 3000 rhythms already in the database, procedurally generated to be good/interesting</span>


      <br><br><strong>user</strong>
      <br>charter<select class="creator" name="creator">

        <option value=""></option>
        <% potentialCreators && potentialCreators.each(function (potentialCreator) { %>
          <% var sel = (creator == potentialCreator.id) ? "selected" : "" %>
          <option value="<%= potentialCreator.id %>" <%= sel %> >
            <%= potentialCreator.escape("email") %>, id <%= potentialCreator.id %>
          </option>
        <% }); %>
      </select>

      <br>liker<select class="liker" name="liker">
        <option value=""></option>

        <% potentialLikers && potentialLikers.each(function (potentialLiker) { %>
          <% var sel = (liker == potentialLiker.id) ? "selected" : "" %>
          <option value="<%= potentialLiker.id %>" <%= sel %> >
            <%= potentialLiker.escape("email") %>, id <%= potentialLiker.id %>
          </option>
        <% }); %>
      </select>

      <br><br><strong>id</strong>
      <br>name<input type="text" class="" value="">
      <br>rhythm string<input type="text" class="" value="">
      <br>durational pattern<input type="text" class="" value="">

      <br><br><strong>distance</strong>
      <br>L<sub>1</sub> norm distance<input type="text" class="l1-distance" value="">
      <br>Euclidian distance<input type="text" class="" value="">
      <br>sup distance<input type="text" class="" value="">
      <br>edit distance<input type="text" class="" value="">
      <br>chronotonic distance<input type="text" class="" value="">

      <br><br><strong>class</strong>
      <br>necklace<input type="text" class="" value="">
      <br>bracelet<input type="text" class="" value="">
      <br>full interval content<input type="text" class="" value="">

      <br><br><strong>contour</strong>
      <br>interval combinatorial class<input type="text" class="" value="">
      <br>superset<input type="text" class="" value="">
      <br>subset<input type="text" class="" value="">
      <br>shadows<input type="text" class="" value="">
      <br>transformational analogs<input type="text" class="" value="">

      <!--<br>rhythm id<input type="text" class="rhythm-id">-->

    </div>

