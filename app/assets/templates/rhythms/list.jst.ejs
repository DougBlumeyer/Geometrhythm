      <i class="glyphicon glyphicon-search"></i><strong style='color=red'>COMING SOON</strong>
      <br>Creator: <select class="creator" name="creator">

        <option value=""></option>
        <% potentialCreators && potentialCreators.each(function (potentialCreator) { %>
          <% var sel = (creator == potentialCreator.id) ? "selected" : "" %>
          <option value="<%= potentialCreator.id %>" <%= sel %> >
            <%= potentialCreator.escape("email") %>, id <%= potentialCreator.id %>
          </option>
        <% }); %>
      </select>

      <br>Liker: <select class="liker" name="liker">
        <option value=""></option>

        <% potentialLikers && potentialLikers.each(function (potentialLiker) { %>
